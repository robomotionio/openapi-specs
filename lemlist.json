        "parameters": [
          {
            "schema": {
              "type": "string",
              "enum": [
                "emailsSent",
                "emailsOpened",
                "emailsClicked",
                "emailsReplied",
                "emailsBounced",
                "emailsSendFailed",
                "emailsUnsubscribed"
              ]
            },
            "in": "query",
            "name": "type",
            "description": "(Optional) The type of activity you want to retrieve. Can be emailsSent, emailsOpened, emailsClicked, emailsReplied, emailsBounced, emailsSendFailed, emailsUnsubscribed."
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "campaignId",
            "description": "(Optional) Retrieve activities of this campaignId."
          },
          {
            "schema": {
              "type": "boolean"
            },
            "in": "query",
            "name": "isFirst",
            "description": "(Optional) Only retrieve the first time this activity happened."
          },
          {
            "schema": {
              "type": "number"
            },
            "in": "query",
            "name": "offset",
            "description": "(Optional) Offset from the start. For pagination."
          },
          {
            "schema": {
              "type": "number"
            },
            "in": "query",
            "name": "limit",
