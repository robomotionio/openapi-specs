    "/api/campaigns/{campaignId}/leads/{email}": {
      "post": {
        "summary": "Add a Lead in a Campaign",
        "operationId": "post-api-campaigns-:campaignId-leads-:email",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "description": "",
                  "type": "object",
                  "properties": {
                    "campaignId": {
                      "type": "string",
                      "minLength": 1
                    },
                    "campaignName": {
                      "type": "string",
                      "minLength": 1
                    },
                    "leadUrl": {
                      "type": "string",
                      "minLength": 1
                    },
                    "_id": {
                      "type": "string",
                      "minLength": 1
                    },
                    "email": {
                      "type": "string",
                      "minLength": 1
                    },
                    "firstName": {
                      "type": "string",
                      "minLength": 1
                    },
                    "lastName": {
                      "type": "string",
                      "minLength": 1
                    }
                  },
                  "required": [
                    "campaignId",
                    "campaignName",
                    "leadUrl",
                    "_id",
                    "email",
                    "firstName",
                    "lastName"
                  ],
                  "x-examples": {
                    "example-1": {
                      "campaignId": "cam_aa7uvyxECcni5KXBM",
                      "campaignName": "Campaign1",
                      "leadUrl": "https://api.lemlist.com/api/leads/richard%40piedpiper.com",
                      "_id": "lea_aaNfSAHJoa4gj86Px",
                      "email": "richard@piedpiper.com",
                      "firstName": "Richard",
                      "lastName": "Hendricks"
                    }
                  }
                }
              }
            }
          }
        },
        "description": "This endpoint adds a lead in a specific campaign. If the lead doesn't exist, it'll be created, then inserted to the campaign. The creator of the lead is the campaign's sender\n\nYou can just add the email without any body.",
        "parameters": [
          {
            "schema": {
              "type": "boolean",
              "default": true
            },
            "in": "query",
            "name": "deduplicate",
            "description": "search email address in another campaign, will not insert the lead if email address already inserted"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "description": "",
                "type": "object",
                "x-examples": {
                  "example-1": {
                    "firstName": "Richard",
                    "lastName": "Hendricks"
                  }
                },
                "properties": {
                  "firstName": {
                    "type": "string",
                    "minLength": 1,
                    "description": "First name of the lead."
                  },
                  "lastName": {
                    "type": "string",
                    "minLength": 1,
                    "description": "Last name of the lead."
                  },
                  "companyName": {
                    "type": "string",
                    "description": "Company name of the lead."
                  }
                },
                "required": [
                  "firstName",
                  "lastName"
                ]
              }
            }
          },
          "description": "Body is optionnal"
        }
      },
      "parameters": [
        {
          "schema": {
            "type": "string"
          },
          "name": "campaignId",
          "in": "path",
          "required": true,
          "description": "The ID of the campaign to add the lead."
        },
        {
          "schema": {
            "type": "string"
          },
          "name": "email",
          "in": "path",
          "required": true,
          "description": "email address of the lead"
        }
      ],
      "patch": {
        "summary": "Update a Lead in a Campaign",
        "operationId": "patch-api-campaigns-campaignId-leads-email",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {}
                }
              }
            }
          },
          "404": {
            "description": "Not Found"
          }
        },
        "description": "This endpoint updates a lead in a specific campaign.\n\nIf the lead doesn't exist a 404 error will be returned.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "description": "",
                "type": "object",
                "x-examples": {
                  "example-1": {
                    "firstName": "Richard",
                    "lastName": "Hendricks"
                  }
                },
                "properties": {
                  "firstName": {
                    "type": "string",
                    "description": "First name of the lead."
                  },
                  "lastName": {
                    "type": "string",
                    "description": "Last name of the lead."
                  },
                  "companyName": {
                    "type": "string",
                    "description": "Company name of the lead."
                  }
                }
              }
            }
          },
          "description": "Body is mandatory. It must be a JSON object with any information you want to update on the lead.\n\nIf no body is specified a 400 error will be returned."
        }
      },
      "delete": {
        "summary": "Unsubscribe a Lead from a Campaign",
        "operationId": "delete-api-campaigns-campaignId-leads-email",
        "responses": {
          "200": {
            "description": "OK"
          }
        },
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "action",
            "description": "Force the deletion of the lead. Value must be \"remove\" if used"
          }
        ],
        "description": "This endpoint delete a lead from a specific campaign.\nAll information, including statistics, will be deleted."
      }
    },
