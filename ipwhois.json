{
  "openapi": "3.0.0",
  "info": {
    "title": "ipwhois.io",
    "version": "1.0",
    "description": "This API service is designed for quick and easy visitors IP geolocation integration into your script or website. Get rid of setting up local GeoIP libraries and forget about regular updates. Our neural network analyzes dozens of sources and almost real-time updates the database.",
    "x-logo": {
      "url": "https://logo.clearbit.com/ipwhois.app"
    }
  },
  "servers": [
    {
      "url": "https://ipwhois.app"
    }
  ],
  "paths": {
    "/json/{IP}": {
      "parameters": [
        {
          "schema": {
            "type": "string",
            "example": "8.8.8.8"
          },
          "name": "IP",
          "in": "path",
          "required": true,
          "description": "can be an IPv4 or IPv6 address, or none to use the current IP address."
        }
      ],
      "get": {
        "summary": "Return geolocation from IP",
        "tags": [
          "geolocation"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "description": "",
                  "type": "object",
                  "properties": {
                    "ip": {
                      "description": "IP used for the query (e.g. 8.8.4.4)",
                      "type": "string"
                    },
                    "success": {
                      "type": "boolean",
                      "description": "true or false"
                    },
                    "type": {
                      "type": "string",
                      "minLength": 1,
                      "description": "IP address type (IPv4 or IPv6)"
                    },
                    "continent": {
                      "type": "string",
                      "minLength": 1,
                      "description": "Continent name (e.g. North America)"
                    },
                    "continent_code": {
                      "type": "string",
                      "minLength": 1,
                      "description": "Two-letter continent code (e.g. NA)"
                    },
                    "country": {
                      "type": "string",
                      "minLength": 1,
                      "description": "Country name (e.g. United States)"
                    },
                    "country_code": {
                      "type": "string",
                      "minLength": 1,
                      "description": "Two-letter country code (e.g. US)"
                    },
                    "country_flag": {
                      "type": "string",
                      "minLength": 1,
                      "description": "The capital of country (e.g. Washington)"
                    },
                    "country_capital": {
                      "type": "string",
                      "minLength": 1
                    },
                    "country_phone": {
                      "type": "string",
                      "minLength": 1
                    },
                    "country_neighbours": {
                      "type": "string",
                      "minLength": 1
                    },
                    "region": {
                      "type": "string",
                      "minLength": 1
                    },
                    "city": {
                      "type": "string",
                      "minLength": 1
                    },
                    "latitude": {
                      "type": "number"
                    },
                    "longitude": {
                      "type": "number"
                    },
                    "asn": {
                      "type": "string",
                      "minLength": 1
                    },
                    "org": {
                      "type": "string",
                      "minLength": 1
                    },
                    "isp": {
                      "type": "string",
                      "minLength": 1
                    },
                    "timezone": {
                      "type": "string",
                      "minLength": 1
                    },
                    "timezone_name": {
                      "type": "string",
                      "minLength": 1
                    },
                    "timezone_dstOffset": {
                      "type": "number"
                    },
                    "timezone_gmtOffset": {
                      "type": "number"
                    },
                    "timezone_gmt": {
                      "type": "string",
                      "minLength": 1
                    },
                    "currency": {
                      "type": "string",
                      "minLength": 1
                    },
                    "currency_code": {
                      "type": "string",
                      "minLength": 1
                    },
                    "currency_symbol": {
                      "type": "string",
                      "minLength": 1
                    },
                    "currency_rates": {
                      "type": "number"
                    },
                    "currency_plural": {
                      "type": "string",
                      "minLength": 1
                    },
                    "completed_requests": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "type",
                    "continent",
                    "continent_code",
                    "country",
                    "country_code",
                    "country_flag",
                    "country_capital",
                    "country_phone",
                    "country_neighbours",
                    "region",
                    "city",
                    "latitude",
                    "longitude",
                    "asn",
                    "org",
                    "isp",
                    "timezone",
                    "timezone_name",
                    "timezone_dstOffset",
                    "timezone_gmtOffset",
                    "timezone_gmt",
                    "currency",
                    "currency_code",
                    "currency_symbol",
                    "currency_rates",
                    "currency_plural",
                    "completed_requests"
                  ]
                },
                "examples": {
                  "example": {
                    "value": {
                      "ip": "8.8.4.4",
                      "success": true,
                      "type": "IPv4",
                      "continent": "North America",
                      "continent_code": "NA",
                      "country": "United States",
                      "country_code": "US",
                      "country_flag": "https://cdn.ipwhois.io/flags/us.svg",
                      "country_capital": "Washington",
                      "country_phone": "+1",
                      "country_neighbours": "CA,MX,CU",
                      "region": "New Jersey",
                      "city": "Newark",
                      "latitude": 40.735657,
                      "longitude": -74.1723667,
                      "asn": "AS15169",
                      "org": "Google LLC",
                      "isp": "Google LLC",
                      "timezone": "America/New_York",
                      "timezone_name": "Eastern Standard Time",
                      "timezone_dstOffset": 0,
                      "timezone_gmtOffset": -18000,
                      "timezone_gmt": "GMT -5:00",
                      "currency": "US Dollar",
                      "currency_code": "USD",
                      "currency_symbol": "$",
                      "currency_rates": 1,
                      "currency_plural": "US dollars",
                      "completed_requests": 0
                    }
                  }
                }
              }
            }
          }
        },
        "operationId": "get-json-IP",
        "parameters": [
          {
            "schema": {
              "type": "string",
              "enum": [
                "en",
                "ru",
                "de",
                "es",
                "pt-BR",
                "fr",
                "zh-CN",
                "ja"
              ],
              "example": "fr",
              "default": "en"
            },
            "in": "query",
            "name": "lang",
            "description": "Localized city, region, country and continent can be requested by setting the GET parameter lang to one of the following:"
          },
          {
            "schema": {
              "type": "string",
              "example": "country,city,timezone"
            },
            "in": "query",
            "name": "objects",
            "description": "You can restrict API results to specific data fields if you donâ€™t need to keep track of all the data. In this case, you need to add the GET objects parameter to the API request's URL and set a specific field or fields list with a comma."
          }
        ],
        "description": "Get geolocation from an IP address, either IPV4 or IPV6",
        "cache": {
          "expire": "604800"
        }
      }
    }
  },
  "components": {}
}